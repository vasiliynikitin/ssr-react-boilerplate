  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Start",
      "program": "${workspaceFolder}\\scripts\\start.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "start dist server",
      "program": "${workspaceFolder}\\dist\\server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Build",
      "program": "${workspaceFolder}\\scripts\\build.js",
      "env": {
        "NODE_ENV": "production"
      }
    },

  ]
}